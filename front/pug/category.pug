            .uk-container
                .uk-grid(uk-grid)
                    .menu(class="uk-width-1-4@l uk-width-1-4@m uk-width-1-3@s uk-width-1-1")
                        .uk-card.uk-card-default
                            .find-category
                                form(action="")
                                    .find
                                        input.finding.uk-input.uk-input-default(type="text", placeholder = "Найти категорию")
                                    .checks
                                        input(type="checkbox", id = "select-cat1") 
                                        label(for="select-cat1") Красота <br>
                                        input(type="checkbox", id = "select-cat2")
                                        label(for="select-cat2") Медицина <br>
                                        input(type="checkbox", id = "select-cat3")
                                        label(for="select-cat3") Образование <br>
                                        input(type="checkbox", id = "select-cat4")
                                        label(for="select-cat4") Ремонт личных вещей <br>
                                        input(type="checkbox", id = "select-cat5")
                                        label(for="select-cat5") Работы по дому <br>
                                        input(type="checkbox", id = "select-cat6")
                                        label(for="select-cat6") Организация мероприятий <br>
                                        input(type="checkbox", id = "select-cat7") 
                                        label(for="select-cat7") Профессиональные услуги <br>
                                        input(type="checkbox", id = "select-cat8")
                                        label(for="select-cat8") Ремонт авто <br>
                                        input(type="checkbox", id = "select-cat9")
                                        label(for="select-cat9") Электроприборы <br>
                                        input(type="checkbox", id = "select-cat10")
                                        label(for="select-cat10") Фото, видео, аудио <br>
                                        input(type="checkbox", id = "select-cat11")
                                        label(for="") Спорт <br>
                                        input(type="checkbox", id = "select-cat12")
                                        label(for="select-cat12") Строительство и ремонт
                                    .submit.uk-text-center
                                        button.uk-button.uk-button-default.uk-margin-top(type="submit") Показать результаты

